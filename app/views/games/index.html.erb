<h1>Games#index</h1>
<p>Find me in app/views/games/index.html.erb</p>

<p>
  <%= link_to 'Back to dashboard', root_path %>
</p>
<p>
  <%= link_to 'See the teams', teams_path %>
</p>

<p>
  <%= link_to 'Create a new game', new_game_path %>
</p>

<hr />

<% @games.each do |game| %>
  <hr />
  <p><%= link_to "#{game.name}", edit_game_path(game) %></p>
  <p><%= game.schedule_date %></p>
  <p><%= game.team_ids %></p>
  <hr />
<% end %>
