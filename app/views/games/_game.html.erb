<%= turbo_frame_tag dom_id(game) do %>
  <div class="card card-body">
    _game.html.erb
    <div><%= game.name %></div>
    <div>
      <% game.scores.each do |score| %>
        <p><%= score.team_name %>: <%= score.points %> points</p>
      <% end %>
    </div>
    <div><%= link_to 'Edit', edit_game_path(game) %></div>
  </div>
<% end %>
